{
  "name": "frontend",
  "version": "0.0.36-alpha",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen localhost:4173",
    "test:mutation": "stryker run",
    "test:mutation:incremental": "stryker run --incremental",
    "test:accessibility": "vitest run tests/integration/accessibility",
    "test:performance": "vitest run tests/performance",
    "test:integration": "vitest run tests/integration",
    "test:visual": "percy exec -- playwright test --project=chromium",
    "test:size": "size-limit",
    "test:all": "run-s test:unit:run test:e2e test:size",
    "test:quick": "vitest run --reporter=basic",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/"
  },
